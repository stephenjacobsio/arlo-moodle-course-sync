{
  "name": "arlo-moodle-course-sync",
  "version": "1.0.0",
  "description": "A Node.js application that syncs courses between Arlo and Moodle via webhooks.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "docs": "typedoc --out docs src"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-retry": "^4.5.0",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-validator": "^7.2.0",
    "joi": "^17.6.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "typedocs": "^0.5.0",
    "uninstall": "^0.0.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.13",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nock": "^13.5.5",
    "prettier": "^3.3.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module"
}
