# Moodle API URL
MOODLE_API_URL=your_moodle_api_url

# Moodle Web Service token
MOODLE_API_TOKEN=your_moodle_api_token

# Moodle Web Service function for course creation
MOODLE_WS_FUNCTION=core_course_create_courses

# Moodle API response format (e.g., json)
MOODLE_WS_FORMAT=json

# Database URL (if applicable)
DATABASE_URL=your_database_url

# Port for running the server
PORT=3000

# Logging level (info, debug, error, etc.)
LOG_LEVEL=info

# Node environment (development, production, test)
NODE_ENV=development